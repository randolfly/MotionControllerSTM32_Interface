@using MotionInterface.Lib.Model
@using MotionInterface.Lib.Util

<MListItem>
    <MContainer>
        <MRow>
            <MCol Cols="6" Style="display: flex; justify-content: flex-start">
                <MChip Label>
                    @ProtocolFrame.Command
                </MChip>
            </MCol>
            <MCol>
                <MCard>
                    @ProtocolFrame.ParamData.ToFloatString()
                </MCard>
            </MCol>
        </MRow>
    </MContainer>

</MListItem>

@code {
    [Parameter] 
    public ProtocolFrame ProtocolFrame { get; set; } = new();
}